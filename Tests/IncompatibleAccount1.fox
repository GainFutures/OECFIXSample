////	Sends Equity orders with Future and Forex accounts. //order1 = New BUY 1 MSFT LMT 1 for prop:FutureAccount;	report11 = Wait ExecutionReport, ClOrdID == order1.ClOrdID;	Ensure report11 != null, 'ExecReport not received';	if (report11.OrdStatus == FIX.OrdStatus.PENDING_NEW)	{		EnsureOrderStatus report11 FIX.OrdStatus.PENDING_NEW;		report12 = Wait ExecutionReport, ClOrdID == order1.ClOrdID;		Ensure report12 != null, 'ExecReport not received';		EnsureOrderStatus report12 FIX.OrdStatus.REJECTED;	}	else	{		EnsureOrderStatus report11 FIX.OrdStatus.REJECTED;	};	order2 = New BUY 1 MSFT LMT 1 for prop:ForexAccount;	report21 = Wait ExecutionReport, ClOrdID == order2.ClOrdID;	Ensure report21 != null, 'ExecReport not received';	if (report21.OrdStatus == FIX.OrdStatus.PENDING_NEW)	{		EnsureOrderStatus report21 FIX.OrdStatus.PENDING_NEW;		report22 = Wait ExecutionReport, ClOrdID == order2.ClOrdID;		Ensure report22 != null, 'ExecReport not received';		EnsureOrderStatus report22 FIX.OrdStatus.REJECTED;	}	else	{		EnsureOrderStatus report21 FIX.OrdStatus.REJECTED;	};